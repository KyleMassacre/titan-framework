{
    "name": "pbbg-io/titan-framework",
    "description": "A description for titan-framework.",
    "type": "package",
    "license": "MIT",
    "keywords": [
        "laravel"
    ],
    "authors": [
        {
            "name": "Ian",
            "email": "ian@pbbg.io"
        }
    ],
    "require": {
        "illuminate/support": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "PbbgIo\\TitanFramework\\": "./src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PbbgIo\\TitanFramework\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "PbbgIo\\TitanFramework\\TitanFrameworkServiceProvider"
            ]
        }
    },
    "require-dev": {
        "orchestra/testbench": "^4.0"
    }
}
